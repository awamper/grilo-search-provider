<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="gnome-shell-extensions">

  <schema path="/org/gnome/shell/extensions/grilo-search-provider/" id="org.gnome.shell.extensions.grilo-search-provider">

    <key type="i" name="search-timeout">
        <default>1000</default>
    </key>

    <key type="i" name="max-results">
        <default>12</default>
    </key>

    <key type="i" name="max-max-results">
        <default>50</default>
    </key>

    <key type="s" name="keyword">
      <default>"g"</default>
    </key>

    <key type="b" name="enable-youtube">
      <default>true</default>
    </key>

    <key type="b" name="enable-vimeo">
      <default>true</default>
    </key>

    <key type="b" name="enable-flickr">
      <default>true</default>
    </key>

    <key type="i" name="results-padding-left">
      <default>20</default>
    </key>

    <key type="i" name="results-padding-right">
      <default>20</default>
    </key>

    <key type="i" name="results-padding-top">
      <default>20</default>
    </key>

    <key type="i" name="results-padding-bottom">
      <default>20</default>
    </key>

    <key type="i" name="results-spacing">
      <default>5</default>
    </key>

    <key type="b" name="remember-last-search">
      <default>true</default>
    </key>

    <key type="s" name="thumbnails-size">
      <default>'Medium'</default>
    </key>

    <key type="b" name="always-show-description">
      <default>false</default>
    </key>

    <key type="b" name="search-on-enter">
      <default>true</default>
    </key>

    <key type="i" name="description-height-percents">
      <default>55</default>
    </key>

  </schema>
</schemalist>
